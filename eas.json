{
  "cli": {
    "version": ">= 16.18.0"
  },
  "build": {
    "production": {
      "channel": "production"
    },
    "apk": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "aab": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "preview": {
      "channel": "preview"
    },
    "development": {
      "channel": "development",
      "developmentClient": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account-key.json",
        "track": "internal"
      }
    }
  }
}
